<Script>local files_to_extract = { 'shell-master.zip', 'framework-master.zip' };

function print ( txt )
	Dialog.Message('', tostring(txt))
end

function addItem ( txt )
	return ListBox.AddItem("ListBox1", txt);
end

ListBox.DeleteItem("ListBox1", -1);

local folder_dest = Input.GetText("Input1");

Folder.DeleteTree(_SourceFolder .. '\\temp');

Folder.Create(_SourceFolder .. '\\temp');
Folder.Create(folder_dest);
Folder.Create(folder_dest.. '\\libraries\\lide');

for _, filename in pairs(files_to_extract) do
	local _nExtracting = addItem('Extracting ' .. filename);
	Application.Sleep(100)
	
	lide.zip.extract (_SourceFolder .. '\\'..filename, _SourceFolder .. '\\temp');
end

addItem ''
addItem ('Copying files from "temp" to ' .. folder_dest);

Application.Sleep(100)

lide_folder_copy(_SourceFolder .. '\\temp\\shell-master', folder_dest);
lide_folder_copy(_SourceFolder .. '\\temp\\framework-master', folder_dest..'\\libraries\\lide');

addItem ''
addItem ''

addItem 'Successfull installed'</Script>
