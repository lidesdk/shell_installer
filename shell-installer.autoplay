<Globals>
<Script>require 'lide.base.init'

lide.zip = require 'lide_zip'

normalize_path = lide.platform.normalize_path

	function lide_file_copy ( src_file, dst_file )
		
		assert(lide.lfs.attributes(src_file).mode == 'file', 'arg #1 must be filepath')
		
		local file_src     = io.open(src_file, 'rb')
		local file_content = file_src : read '*a'
		--lide.lfs.lock(file_src)
		-- only copy new files if file dooesn exist
		if not lide.core.file.doesExists( dst_file ) then
			local file_dst = io.open(dst_file, 'w+b')
			file_dst:write(file_content)
			file_dst:flush()
			file_src:close()

			file_dst:close()
		end
		--lide.lfs.unlock(file_src)
	end

-- funcion para copiar los archivos de una carpeta recursivamente
function lide_folder_copy( src_folder, dst_folder )		
	for file in lide.lfs.dir( src_folder ) do
		if (file ~= '.' and file ~= '..') then
			local _src_path  = normalize_path(src_folder .. '/' .. file);
			local _dst_path  = normalize_path(dst_folder ..'/'.. file);
			
			-- file is the current file or directory name
			if lide.lfs.attributes (_src_path) . mode == 'directory' then
	    		lide.lfs.mkdir(_dst_path);
	    		-- use this function to copy recursively
	    		lide_folder_copy(_src_path, _dst_path);
			else
				lide_file_copy(_src_path, _dst_path);
			end
		end
	end
end</Script>
